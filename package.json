{
  "name": "guia-hoteles",
  "version": "1.0.0",
  "description": "'proyecto xxx'",
  "main": "index.html",
  "dependencies": {
    "boostrap": "^2.0.0",
    "bootstrap": "^4.5.2",
    "concurrently": "^5.3.0",
    "jquery": "^3.5.1",
    "lite-server": "^2.5.4",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1",
    "sas": "^3.0.4",
    "sass": "^1.26.10"
  },
  "devDependencies": {
    "cssmin": "^0.4.3",
    "dev": "lite-server",
    "htmlmin": "0.0.7",
    "imagemin": "^7.0.1",
    "imagemin-cli": "^6.0.0",
    "node-sass": "^4.14.1",
    "onchange": "^7.0.2",
    "rimraf": "^3.0.2",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  },
  "scripts": {
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev \"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "clean": "rimraf dist",
    "imagemin": "imagemin imagenes/* --out-dir dist/images",
    "usemin":"usemin index.html -d dist --htmlmin -o dist/index.html && usemin contacto.html -d dist --htmlmin -o dist/contacto.html && usemin precios.html -d dist --htmlmin -o dist/precios.html && usemin about.html -d dist --htmlmin -o dist/about.html",
     "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://fercadev@bitbucket.org/fercadev/guia-hoteles.git"
  },
  "author": "fercadev",
  "license": "ISC",
  "homepage": "https://bitbucket.org/fercadev/guia-hoteles#readme"
}
